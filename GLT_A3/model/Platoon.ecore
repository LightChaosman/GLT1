<?xml version="1.0" encoding="UTF-8"?>
<ecore:EPackage xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:ecore="http://www.eclipse.org/emf/2002/Ecore" name="">
  <eSubpackages name="platoon" nsURI="platoon" nsPrefix="platoon">
    <eClassifiers xsi:type="ecore:EClass" name="Route">
      <eStructuralFeatures xsi:type="ecore:EReference" name="commands" lowerBound="1"
          upperBound="-1" eType="#//platoon/Command" containment="true"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="Command" abstract="true"/>
    <eClassifiers xsi:type="ecore:EClass" name="Forward" eSuperTypes="#//platoon/Command">
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="distance" lowerBound="1"
          eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EInt"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="Turn" abstract="true" eSuperTypes="#//platoon/Command"/>
    <eClassifiers xsi:type="ecore:EClass" name="TurnLeft" eSuperTypes="#//platoon/Turn"/>
    <eClassifiers xsi:type="ecore:EClass" name="TurnRight" eSuperTypes="#//platoon/Turn"/>
    <eClassifiers xsi:type="ecore:EClass" name="Platoon">
      <eStructuralFeatures xsi:type="ecore:EReference" name="leader" lowerBound="1"
          eType="#//platoon/LeadVehicle"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="followers" upperBound="-1"
          eType="#//platoon/FollowVehicle"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="Vehicle" abstract="true"/>
    <eClassifiers xsi:type="ecore:EClass" name="LeadVehicle" eSuperTypes="#//platoon/Vehicle">
      <eStructuralFeatures xsi:type="ecore:EReference" name="route" lowerBound="1"
          eType="#//platoon/Route"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="FollowVehicle" eSuperTypes="#//platoon/Vehicle">
      <eStructuralFeatures xsi:type="ecore:EReference" name="frontrunner" lowerBound="1"
          eType="#//platoon/Vehicle"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="Constraints">
      <eStructuralFeatures xsi:type="ecore:EReference" name="constraints" upperBound="-1"
          eType="#//platoon/Constraint"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="Constraint" abstract="true"/>
    <eClassifiers xsi:type="ecore:EClass" name="headway" eSuperTypes="#//platoon/Constraint">
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="lowbound" lowerBound="1"
          eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EInt"/>
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="upbound" lowerBound="1"
          eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EInt"/>
    </eClassifiers>
  </eSubpackages>
</ecore:EPackage>
